{
  "name": "micro-catalog",
  "version": "1.0.0",
  "description": "codeeducation microservice devops",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t micro-catalog .",
    "docker:run": "docker run -p 3000:3000 -d micro-catalog",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test",
    "console": "node dist/console.js"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.3.2",
    "@loopback/context": "^3.8.2",
    "@loopback/core": "^2.7.1",
    "@loopback/openapi-v3": "^3.4.2",
    "@loopback/repository": "^2.6.0",
    "@loopback/rest": "^5.1.0",
    "@loopback/rest-crud": "^0.8.6",
    "@loopback/rest-explorer": "^2.2.3",
    "@loopback/service-proxy": "^2.3.1",
    "amqp-connection-manager": "^3.2.0",
    "amqplib": "^0.5.6",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.19",
    "loopback-connector-esv6": "git+https://github.com/codeedu/loopback-connector-elastic-search.git"
  },
  "devDependencies": {
    "@loopback/build": "^5.4.2",
    "@loopback/eslint-config": "^8.0.0",
    "@loopback/testlab": "^3.1.6",
    "@types/amqp-connection-manager": "^2.0.10",
    "@types/amqplib": "^0.5.13",
    "@types/lodash": "^4.14.152",
    "@types/node": "^10.17.24",
    "@types/swagger-ui-dist": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^7.0.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~3.9.3"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "src/"
    ],
    "ext": ".ts",
    "exec": "npm start"
  }
}
